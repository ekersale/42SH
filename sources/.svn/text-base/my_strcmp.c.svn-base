#include	<stdlib.h>
#include	<unistd.h>
#include	"proto.h"

int		my_strcmp(char *s1, char *s2)
{
  int		i;

  i = 0;
  while (s1[i] && s2[i] && s1[i] == s2[i])
    i++;
  if (!s1[i] && !s2[i])
    return (1);
  return (0);
}

int		my_strcom(char *s1, char *s2)
{
  int		i;

  i = 0;
  while (s1[i] && s2[i] && s1[i] == s2[i])
    i++;
  if (!s2[i])
    return (1);
  return (0);
}
